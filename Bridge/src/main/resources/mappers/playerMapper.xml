<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

   
<mapper namespace="com.bridge.mappers.musicMapper">   
   <select id="music" resultType="MusicVO">
      select * from music as m,artist as a  where m.musicNumber=#{musicNumber} and m.artistnumber=a.artistnumber
   </select>
</mapper>